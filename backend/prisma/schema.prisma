datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  createdAt DateTime @default(now())
  userDocuments Documents[]
}

model Documents{
  id   Int @id @unique
  status String
  json Json 
}
